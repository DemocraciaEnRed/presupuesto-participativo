
.popup-proyectista{
    box-sizing: border-box;
    position: absolute;
    top: 300px;
    width: 250px;
    height: 34px;
    padding: 0 20px;
    margin: 0;
    cursor: pointer;
    background: $primary;
    border-radius: 4px 4px 0px 0px;
    box-shadow: 0px 2px 12px rgba(0, 0, 0, 0.06), 0px 2px 4px rgba(0, 0, 0, 0.08);
    display: flex;
    align-items: center;
    justify-content: flex-start;
    transform: rotate(-90deg);
    transform-origin: bottom left;
    color: white;
    text-decoration: none;
    z-index: 9999;
    transition: all 1s ease-in-out
    border: none
    }
.popup-proyectista.active{
    right: 50px
    }

.text-popup{
    text-decoration: none;
    font-size: 16px;
    font-family: Helvetica,Arial,sans-serif;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 100%;
    text-align: center;
    -webkit-font-smoothing: antialiased;
}

.side-panel{
    position: fixed;
    top: calc(50% - 250px);
    right: 0;
    transition: width 300ms ease-out;
    width: 0;
    }
.side-panel > a:hover{
    color: #fff
    }
    
#typeform-full{
    position: fixed;
    height: 50vh
    top: 50%;
    right: 0;
    max-width: 1000px
    transform: translate(0%, -50%);
  }

#typeform-full.closed{
    right: -100%
    }

.popup-votacion{
    box-sizing: border-box;
    position: absolute;
    top: 300px;
    width: 180px;
    height: 34px;
    padding: 0 20px;
    margin: 0;
    cursor: pointer;
    background: $primary;
    border-radius: 4px 4px 0px 0px;
    box-shadow: 0px 2px 12px rgba(0, 0, 0, 0.06), 0px 2px 4px rgba(0, 0, 0, 0.08);
    display: flex;
    align-items: center;
    justify-content: flex-start;
    transform: rotate(-90deg);
    transform-origin: bottom left;
    color: white!important;
    text-decoration: none;
    z-index: 9999;
}


.glyphicon-comment{
    
    width: 32px;
    position: relative;
    text-align: center;
    transform: rotate(90deg) scale(0.75);
    left: -8px;
}

.typeForm-share{
    box-sizing:border-box;
    position:absolute;
    top:300px;
    width:200px;
    height:48px;
    padding:0 20px;
    margin:0;
    cursor:pointer;
    background:$primary;
    border-radius:4px 4px 0px 0px;
    box-shadow:0px 2px 12px rgba(0, 0, 0, 0.06), 0px 2px 4px rgba(0, 0, 0, 0.08);
    display:flex;
    align-items:center;
    justify-content:flex-start;
    transform:rotate(-90deg);
    transform-origin:bottom left;
    color:white;
    text-decoration:none;
    z-index:9999;
    }

.typeForm-share > span{
     width:32px;
     position:relative;
     text-align:center;
     transform:rotate(90deg) scale(0.85);
     left:-8px;
     }

.typeForm-share > span{
    text-decoration:none;
    font-size:18px;
    font-family:Helvetica,Arial,sans-serif;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
    width:100%;
    text-align:center;
    -webkit-font-smoothing:antialiased;
    -moz-osx-font-smoothing:grayscale
}